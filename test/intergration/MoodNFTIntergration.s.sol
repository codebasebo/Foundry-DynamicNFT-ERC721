pragma solidity ^0.8.20;

import {Test, console} from "forge-std/src/Test.sol";
import {MoodNft} from "src/MoodNFT.sol";
import {deployMoodNFT} from "script/DeployMoodNFT.s.sol";

contract moodNFTTest is Test {
    MoodNft public moodNft;
    address public USER = makeAddr("USER");

    string public constant HAPPY_MOOD_URI =
        "data:image/svg+xml;base64,";
    string public constant SAD_MOOD_URI =
        "data:application/json;base64,";
    deployMoodNFT public deployMoodNft;

    function setUp() public {
        deployMoodNft = new deployMoodNFT();
        moodNft = deployMoodNft.run();
    }

    // function test_mintNft() public {
    //     moodNft.mintNft();
    //     assertEq(moodNft.ownerOf(0), address(this));
    // }

    function testViewTokenURIIntergration() public {
        vm.prank(USER);
        moodNft.mintNft();
        console.log(moodNft.tokenURI(0));
    }

    function testFlipTokenToSad() public {
        vm.prank(USER);
        moodNft.mintNft();

        vm.prank(USER);
        moodNft.flipMood(0);

        assert(
            keccak256(abi.encodePacked(moodNft.tokenURI(1))) ==
                keccak256(abi.encodePacked(SAD_MOOD_URI))
        );
    }
}
